# AgentJobs Configuration
DATABASE_URL=sqlite:///./agentjobs.db
# For PostgreSQL/Supabase:
# DATABASE_URL=postgresql://user:pass@host:5432/agentjobs

# Server
HOST=0.0.0.0
PORT=8000
DEBUG=true

# API
API_PREFIX=/api/v1
DEFAULT_PAGE_SIZE=20
MAX_PAGE_SIZE=100

# Rate Limiting
FREE_RATE_LIMIT=100
PRO_RATE_LIMIT=1000
ENTERPRISE_RATE_LIMIT=10000

# Scraping
SCRAPE_INTERVAL_HOURS=6
MAX_CONCURRENT_SCRAPES=5
